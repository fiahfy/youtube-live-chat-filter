{
  "name": "youtube-live-chat-filter",
  "description": "Chrome Extension for Filtering Chat Messages on YouTube Live.",
  "version": "0.0.12",
  "dependencies": {
    "@vue/composition-api": "^1.7.1",
    "nanoid": "^4.0.2",
    "vue": "~2.6.14",
    "vuetify": "~2.6.15",
    "vuex": "~3.6.2",
    "vuex-module-decorators": "~1.2.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@fiahfy/eslint-config-vue": "^0.0.20",
    "@fiahfy/prettier-config": "^0.0.3",
    "@fiahfy/tsconfig": "^0.0.2",
    "@types/chrome": "^0.0.235",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "mv3-hot-reload": "^0.2.7",
    "prettier": "^2.8.8",
    "sass": "~1.32.13",
    "sass-loader": "^13.2.2",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "vue-loader": "~15.9.8",
    "vue-template-compiler": "~2.6.14",
    "vuetify-loader": "~1.7.3",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "private": true,
  "productName": "Chat Filter for YouTube Live",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently npm:watch:*",
    "format": "prettier -c --ignore-path .gitignore .",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "package": "zip -r extension.zip dist",
    "prepare": "husky install",
    "test": "npm run format && npm run lint",
    "watch:dist": "mv3-hot-reload",
    "watch:src": "webpack -w --config webpack.config.dev.js"
  }
}
